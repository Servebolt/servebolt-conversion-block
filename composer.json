{
    "name": "servebolt/servebolt-wp-optimize-plugin",
    "type": "wordpress-plugin",
    "license": "GPLv3 or later",
    "description": "A WordPress-plugin that will insert conversion blocks into the content of a post.",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Robert SÃ¦ther",
            "email": "robert@servebolt.com"
        }
    ],
    "require": {
        "composer/installers": "~1.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "wp-coding-standards/wpcs": "*",
        "phpcompatibility/php-compatibility": "*",
        "phan/phan": "dev-master"
    },
    "extra": {
        "installer-name": "servebolt-wp-optimize-plugin"
    },
    "keywords": [
        "blocks",
        "editor",
        "gutenberg",
        "plugin",
        "wordpress"
    ],
    "autoload": {
        "psr-4": {
            "Servebolt\\ConversionBlocks\\": "inc"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phan"
        ],
        "post-install-cmd": [
            "[ -f vendor/bin/phpcs ] && \"vendor/bin/phpcs\" --config-set installed_paths vendor/wp-coding-standards/wpcs || true"
        ],
        "post-update-cmd": [
            "[ -f vendor/bin/phpcs ] && \"vendor/bin/phpcs\" --config-set installed_paths vendor/wp-coding-standards/wpcs || true"
        ]
    }
}
